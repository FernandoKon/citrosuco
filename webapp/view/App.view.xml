<mvc:View controllerName="com.lab2dev.citrosuco.controller.App"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">

    <VBox id="VBOXAPP">
        <IconTabBar
        id="iconTabBar"
        select="onTabSelect"
        class="sapUiResponsiveContentPadding sapUiNoContentPadding"
        applyContentPadding="false"
        >
        <layoutData>
            <OverflowToolbarLayoutData priority="NeverOverflow" shrinkable="true"/>
        </layoutData>
        <items>
            <IconTabFilter key="Home" text="Caixas Contratadas"/>
            <IconTabFilter key="Partners" text="Sócios"/>
            <IconTabFilter key="Contracts" text="Contratos Transferidos"/>
            <IconTabFilter key="Portion" text="Evento Parcela"/>
            <IconTabFilter key="Event" text="Evento Contra Entrega"/>
            <IconTabFilter key="Quota" text="Cota"/>
            <IconTabFilter key="Credit" text="Extrator Crédito"/>
            <IconTabFilter key="Debit" text="Extrator Débito"/>
            <IconTabFilter key="Fruit" text="Plano Macro Processamento de Fruta"/>
            <IconTabFilter key="TotalFlowBoxes" text="Caixas Totais Fluxo"/>
            <IconTabFilter key="FruitCadence" text="Cadência de Frutas"/>
            <IconTabFilter key="Params" text="Parâmetro"/>
        </items>
    </IconTabBar>

    <App id="app">
    </App>
    </VBox>
</mvc:View>