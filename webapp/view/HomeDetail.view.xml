<mvc:View
    controllerName="com.lab2dev.citrosuco.controller.HomeDetail"
	height="100%"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
    xmlns:layout="sap.ui.layout"
