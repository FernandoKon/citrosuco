    xmlns:ff="sap.ui.layout.form"   
    xmlns:core="sap.ui.core"
    xmlns:main="sap.ui.webc.main">
	<f:DynamicPage id="OrderDetailPage" showNavButton="true" navButtonPress="onNavBack">
