							<ObjectAttribute title="Rack" text="34"/>
						</layout:VerticalLayout>
                        <layout:VerticalLayout class="sapUiMediumMarginEnd">
							<ObjectAttribute title="Location" text="Warehouse A"/>
							<ObjectAttribute title="Halway" text="23L"/>
							<ObjectAttribute title="Rack" text="34"/>
						</layout:VerticalLayout>
                        <layout:VerticalLayout class="sapUiMediumMarginEnd">
							<ObjectAttribute title="Location" text="Warehouse A"/>
							<ObjectAttribute title="Halway" text="23L"/>
							<ObjectAttribute title="Rack" text="34"/>
						</layout:VerticalLayout>
					</layout:HorizontalLayout>
				</f:DynamicPageHeader>
			</f:header>


        <f:content>
            <VBox fitContainer="true">
                <Table
                    id="idProductsTable"
                    sticky="HeaderToolbar,ColumnHeaders"
                    inset="false"
                    items=""
                    class="sapFDynamicPageAlignContent"
                    width="auto">
                    <headerToolbar>
                        <Toolbar>
                            <Title text="Sócios desse fornecedor" level="H2"/>
                        </Toolbar>
                    </headerToolbar>
                    <columns>
                        <Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
                            <Text text="Código" />
                        </Column>
                        <Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
                            <Text text="Nome" />
                        </Column>
                        <Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
                            <Text text="Descrição" />
                        </Column>
						<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
                            <Text text="Função" />
                        </Column>
						<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
                            <Text text="Descrição 2" />
                        </Column>
						<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
                            <Text text="CPF" />
                        </Column>
						<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
                            <Text text="RG" />
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem>
                            <cells>
                                <ObjectIdentifier/>
                                <ObjectNumber/>
                                <Text />
                                <ObjectNumber/>
                            </cells>
                        </ColumnListItem>
                    </items>
